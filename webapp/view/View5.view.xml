                    </f:content>
                </f:SimpleForm>
            </Panel>
            <Panel id="idPanelTableFour" headerText="InspecciÃ³n de piezas">
                <Table id="idProductsTableFour" inset="false" items="{jsonModelConfig>/RellenoTabla}">
